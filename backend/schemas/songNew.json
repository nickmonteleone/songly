{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.playlist.com/songNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "playlistHandle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "artist": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "link": {
      "type": "string",
      "maxLength": 500
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "playlistHandle"
  ]
}
